<% if @event_ticket_detail_views.count.zero? %>
  <div class="row main-column" id="objects">
    <strong>No Ticket Views Found</strong>
  </div>
<% else %>

  <div class="row main-column" id="objects">
    <table class="data table table-striped no-margin">
      <thead>
      <tr>
        <th> Viewed By</th>
        <th> Timestamp</th>
      </tr>
      </thead>
      <tbody>
      <% @event_ticket_detail_views.each do |ticket_view| %>
        <tr>
          <td><%= ticket_view.user.name %></td>
          <td><%= ticket_view.created_at.strftime(Api::Emails::DEFAULT_TIME_FORMAT) %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
  <div class="row main-column">
    <div id="paginator" class="pull-right">
      <%= will_paginate @event_ticket_detail_views, renderer: BootstrapPagination::Rails, param_name: 'event_ticket_detail_views_page', params: {scope: 'event_ticket_detail_views'} %>
    </div>
  </div>

<% end %>